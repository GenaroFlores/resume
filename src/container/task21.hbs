<main>
     <div class="card">
        <div class="card-body">
            {{#with heading}}
                <div class="card">
                    <div class="card-body"> 
                        {{#with main}}
                            <div>
                                <h1>{{title}}</h1>
                            </div>
                            <br>
                            <p>
                                Description: <b>{{text}}</b>
                            </p>
                            <br>
                            {{#with button-text}}
                                <button class="btn btn-primary">
                                    {{solid}}
                                </button>
                                <br>
                                <br>
                                <button class="btn btn-outline-primary">
                                    {{outline}}
                                </button>
                            {{/with}}
                        {{/with}}
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        <div class="card-body"> 
                            {{#with second}}
                                <div>
                                    <h2>{{title}}</h2>
                                </div>
                                <br>
                                <p>
                                    Description: <b>{{text}}</b>
                                </p>
                                <br>
                                {{#with button-text}}
                                    <button class="btn btn-secondary">
                                        {{solid}}
                                    </button>
                                    <br>
                                    <br>
                                    <button class="btn btn-outline-secondary">
                                        {{outline}}
                                    </button>
                                {{/with}}
                            {{/with}}
                        </div>
                    </div>
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        <div class="card-body"> 
                            {{#with subblock}}
                                <div>
                                    <h3>{{title}}</h3>
                                </div>
                                <br>
                                <p>
                                    Description: <b>{{text}}</b>
                                </p>
                                <br>
                                {{#with button-text}}
                                    <button class="btn btn-dark">
                                        {{solid}}
                                    </button>
                                    <br>
                                    <br>
                                    <button class="btn btn-outline-dark">
                                        {{outline}}
                                    </button>
                                {{/with}}
                            {{/with}}
                        </div>
                    </div>
                </div>
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with nested_display}}
                <h1 class="display-1">
                    {{title}}
                </h1>
                <br>
                <div class="card">
                    <div class="card-body">
                        {{#with children_display}}
                            <div class="display-4">
                                <b>{{title}}</b>
                            </div>
                            <p>
                                Description: <b>{{text}}</b>
                            </p>
                            <button class="btn btn-outline-warning">
                                {{#with button}}
                                    {{text}}
                                {{/with}}
                            </button>
                        {{/with}}
                    </div>
                </div>
                <br>
                <div>
                    <h6>{{header_text}}</h6>
                </div>
                <br>
                <button class="btn btn-info">
                    {{#with button}}
                        {{text}}
                    {{/with}}
                </button>
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with paragraph}}
                <div>
                    <p class="text-uppercase">{{title}}</p>
                </div>
                <div>
                    <p class="text-lowercase">{{text}}</p>
                </div>
                <br>
                <div>
                    {{#with button}}
                        <a class="btn btn-link" href={{url}} target="_blank">
                            {{text}}
                        </a>
                    {{/with}}
                </div>
                <br>
                <div class="card">
                    <div class="card-body">
                        <ul>
                            {{#each list_name}}
                                <li>
                                    <a class="btn btn-link" href={{url}} target="_blank">
                                        {{text}}
                                    </a>
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                </div>
            {{/with}}
        </div>
    </div>
</main>