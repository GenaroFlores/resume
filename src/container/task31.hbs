<main>

    <div class="row">

        <section>
            <nav>
                {{#with navigation}}
                    <div class="container">
                        <div class="row">
                            {{#each links}}
                                <div class="col-2">
                                    <a class="btn btn-link" href="{{href}}">{{text}}</a>
                                </div>
                            {{/each}}
                            <div class="col-2">
                                <a class="btn btn-primary" href="{{button.href}}">{{button.text}}</a>
                            </div>
                       </div>
                    </div>
                {{/with}}
            </nav>
            <hr>
        </section>

        <section>
            {{#with header}}
                <h1>{{title}}</h1>
                <p>{{description}}</p>
                {{#with button}}
                    <a class="btn btn-primary mb-4" href={{href}}>
                        {{text}}
                    </a>
                {{/with}}
            {{/with}}
            <hr>
        </section>

        <section>
            <div>
                {{#with city}}
                    <h2 class="mb-4">{{title}}</h2>
                    <div class="container">
                        <div class="row">
                            {{#each cards}}
                                <div class="card col-5 mx-2 my-3">
                                    {{#with picture}}
                                        <img class="card-img-top" src={{src}} alt={{alt}}>
                                    {{/with}}
                                    <div class="card-header">
                                        <h3>{{title}}</h3>
                                    </div>
                                    <div class="card-body">
                                        <p>{{description}}</p>
                                        {{#with button}}
                                            <a class="btn btn-link" href={{href}}>
                                                {{text}}
                                            </a>
                                        {{/with}}
                                    </div>
                                </div>
                            {{/each}}
                        </div>
                    </div>
                {{/with}}
            </div>
            <br>
            <hr>
        </section>

        <section>
            {{#with winner}}
                <h2>{{title}}</h2>
                {{#with table}}
                    <table class="table mb-5">
                        <thead>
                            <tr>
                                {{#each head}}
                                <th class="text-capitalize">
                                    {{this}}
                                </th>
                                {{/each}}
                            </tr>
                        </thead>
                        {{#each user}}
                            <tbody>
                                <tr>
                                    <th>
                                        {{place}}
                                    </th>
                                    <td>
                                        {{name}} 
                                    </td>
                                    <td>
                                        {{surname}}
                                    </td>
                                    <td>
                                        {{nickname}}
                                    </td>
                                </tr>
                            </tbody>
                        {{/each}}
                    </table>
                {{/with}}
            {{/with}}
            <hr>
        </section>

        <section>
            {{#with gallery}}
                <h2 class="mb-4">{{title}}</h2>
                <div class="container">
                    <div class="row">
                        {{#each cards}}
                            <div class="col-4">
                                <div class="card mb-5">
                                    <div class="card-header">
                                        <h3>{{name}}</h3>
                                    </div>
                                    <ul class="list-group list-group-flush">
                                        {{#each items}}
                                            <li class="list-group-item">{{this}}</li>
                                        {{/each}}
                                    </ul>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            {{/with}}
            <hr>
        </section>

        <section>
            {{#with relaxation}}
                <p class="display-3">{{title}}</p>
                <div class="container">
                    <div class="row">
                        {{#each cards}}
                            <div class="col-5">
                                <div class="card mb-5">
                                    <div class="card-header">
                                        {{header}}
                                    </div>
                                    <div class="card-body">
                                        <h2>{{title}}</h2>
                                        <p>{{description}}</p>
                                    </div>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            {{/with}}
            <hr>
        </section>

        <section>
            {{#with rest}}
                <p class="display-3">{{title}}</p>
                <div class="container">
                    <div class="row">
                        {{#each cards}}
                            <div class="col-6">
                                <div class="card mb-5">
                                    <div class="card-header text-center">
                                        {{header}}
                                    </div>
                                    <div class="card-body text-center">
                                        <h3>{{title}}</h3>
                                        <p>{{description}}</p>
                                        <button class="btn btn-primary text ">
                                            {{button}}
                                        </button>
                                    </div>
                                    <div class="card-footer text-center">
                                        {{footer}}
                                    </div>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            {{/with}}
            <hr>
        </section>
        
        <section>
            {{#with proposition}}
                <p class="display-2">{{title}}</p>
                <div class="container">
                    <div class="row">
                        {{#each cards}}
                            <div class="col-5">
                                <div class="card text-bg-warning text-light mb-5">
                                    <div class="card-header">
                                        {{header}}
                                    </div>
                                    <div class="card-body">
                                        <h3>{{title}}</h3>
                                        <p>{{description}}</p>
                                    </div>
                                </div>
                            </div>
                        {{/each}}
                    </div>
                </div>
            {{/with}}
            <hr>
        </section>
 
        <div class="row">
            {{#with footer}}
                <h2 class="mb-4">{{title}}</h2>
                <div class="col-4 list-group">
                    {{#each navigation}}
                        <a href="{{href}}" class="list-item" target="_blank">
                            <button class="btn btn-link">{{text}}</button>
                        </a>
                        <br>
                    {{/each}}
                </div>

                <div class="col-4 list-group">
                    {{#each socialMedia}}
                        <a href="{{link}}" class="list-item" target="_blank">
                            <button class="btn btn-outline-dark">{{name}}</button>
                        </a>
                        <br>
                    {{/each}}
                </div>

                <div class="col-4 list-group">
                    {{#each partners}}
                        <a href="{{link}}" class="list-item" target="_blank">
                            <button class="btn btn-outline-info">{{name}}</button>
                        </a>
                        <br>
                    {{/each}}
                </div>
            {{/with}}
        </div>

    </div>
</main>

