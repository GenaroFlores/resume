<div>
    {{> title data="Facebook" isBig=true}}
    <hr>
    <main>
        <ul>
            {{#each users}}
                <li>
                    <div>
                         {{> title data="User" isBig=true}}
                        <p>Name: <strong>{{name}}</strong></p>
                        <p>Gender: <b>{{gender}}</b></p>
                        <p>Age: <b>{{age}}</b></p>
                        <p>Identificator: <b>{{id}}</b></p>
                        <hr>
                    </div>
                    <div>
                        {{> title data="Friends" isBig=true}}
                        <ul>
                            {{#each friends}}
                                <li>
                                    <div>
                                        <p>Name: <strong>{{name}}</strong></p>
                                        <p>Gender: <b>{{gender}}</b></p>
                                        <p>Age: <b>{{age}}</b></p>
                                        <p>Identificator: <b>{{id}}</b></p>
                                        <hr>
                                    </div>
                                    <div>
                                        {{> title data="Mutual friends"}}
                                        <ul>
                                            {{#each mutual_friends}}
                                                <li>
                                                    <p>Name: <strong>{{name}}</strong></p>
                                                    <p>Gender: <b>{{gender}}</b></p>
                                                    <p>Age: <b>{{age}}</b></p>
                                                    <p>Identificator: <b>{{id}}</b></p>
                                                    <hr>
                                                </li>
                                            {{/each}}
                                        </ul>
                                    </div>
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                    <div>
                        {{> title data="Groups" idBig=true}}
                        <ul>
                            {{#each groups}}
                                <li>
                                    <div>
                                        <p>Name of group: <strong>{{name}}</strong></p>
                                        <p>Description: <b>{{description}}</b></p>
                                        <p>Identificator: <b>{{id}}</b></p>
                                        <hr>
                                    </div>
                                    <div>
                                        {{> title data="Members"}}
                                        <ul>
                                            {{#each members}}
                                                <li>
                                                    <p>Name: <strong>{{name}}</strong></p>
                                                    <p>Gender: <b>{{gender}}</b></p>
                                                    <p>Age: <b>{{age}}</b></p>
                                                    <p>Identificator: <b>{{id}}</b></p>
                                                    <hr>
                                                </li>
                                            {{/each}}
                                        </ul>
                                    </div>
                                </li>
                            {{/each}}
                        </ul>
                    </div>
                </li>
            {{/each}}
        </ul>
    </main>
</div>