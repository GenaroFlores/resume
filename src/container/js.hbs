<div>
    {{> title data="JavaScript language" isBig=true}}
    <hr>
    <section>
        {{> title data="General"}}
        <br>
        <p>Name: <b>{{name}}</b></p>
        <p>Description: <b>{{description}}</b></p>
    </section>
    <hr>
    <section>
        {{#with history}}
        {{> title data="History"}}
        <br>
        <p>Year founded: <b>{{year}}</b></p>
        <p>Founder name: <b>{{founder}}</b></p>
        {{/with}}
    </section>
    <hr>
    <section>
        {{> title data="Types of data"}}
        <br>
        <ol>
            {{#each types}}
            <li>{{this}}</li>
            {{/each}}
        </ol>
    </section>
    <hr>
    <section>
        <div>
            {{> title data="Syntax rules" isBig=true}}
            <hr>
        </div>
       
        {{#with syntax}}
        {{> title data="Variables"}}
        <ul>
            {{#each variables}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
         {{> title data="Functions:"}}
        <ul>
            {{#each functions}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
         {{> title data="Conditionals:"}}
        <ul>
            {{#each conditionals}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
         {{> title data="Loops:"}}
        <ul>
            {{#each loops}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
         {{> title data="Classes:"}}
        <ul>
            {{#each classes}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        {{/with}}
    </section>
    <hr>
    <section>
        {{> title data="Features"}}
        <br>
        <ol>
            {{#each features}}
            <li>{{this}}</li>
            {{/each}}
        </ol>
    </section>
    <hr>
     <section>
        {{> title data="Libraries:"}}
        <br>
        <ul>
            {{#each libraries}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    </section>
    <hr>
    <section>
        {{> title data="Tools:"}}
        <br>
        <ul>
            {{#each libraries}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
    </section>
    <hr>
    <section>
        {{> title data="Communities:"}}
        <br>
        <ul>
            {{#each community}}
            <li>
                <p>Name: <b>{{name}}</b></p>
                <p>Type: <b>{{type}}</b></p>
            </li>
            {{/each}}
        </ul>
    </section>
</div>